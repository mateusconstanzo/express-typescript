{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "serve": "nodemon ./dist/index.js",
    "start": "npm install && concurrently \"npm run build:watch\" \"npm run serve\"",
    "test": "env TS_NODE_PROJECT=src nyc mocha --reporter spec --compilers ts:ts-node/register 'src/test/**/*.test.ts'"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 90,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/test/"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "extension": [
      ".ts"
    ],
    "cache": true,
    "all": true
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "dependencies": {
    "body-parser": "^1.18.2",
    "class-validator": "^0.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "json-typescript-mapper": "^1.1.3",
    "memory-cache": "^0.2.0",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "typeorm": "^0.1.7"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^3.4.34",
    "@types/chai-http": "0.0.29",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.0.39",
    "@types/memory-cache": "^0.2.0",
    "@types/mocha": "^2.2.32",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.0.57",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.5.1",
    "debug": "^3.1.0",
    "mocha": "^3.1.2",
    "mocha-typescript": "^1.1.12",
    "nodemon": "^1.12.5",
    "nyc": "^11.3.0",
    "supertest": "^3.0.0",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^2.6.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
